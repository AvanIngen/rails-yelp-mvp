<h4>Things that went wrong...</h4>
<ul>
  <% @review.errors.full_messages.each do |message| %>
    <li> <%= message %> </li>
  <% end %>
</ul>

<h1>Review <%= @restaurant.name %></h1>

<%= simple_form_for([@restaurant, @review]) do |f| %>
  <%= f.input :content %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
